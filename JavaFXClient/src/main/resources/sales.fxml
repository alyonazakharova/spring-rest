<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: LINEN;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.SalesController">
    <children>
        <TableView fx:id="salesTable" layoutX="14.0" layoutY="25.0" prefHeight="350.0" prefWidth="383.0">
            <columns>
                <TableColumn fx:id="salesIdColumn" prefWidth="64.0" text="id" />
                <TableColumn fx:id="salesGoodIdColumn" prefWidth="64.0" text="good_id" />
                <TableColumn fx:id="salesGoodCountColumn" prefWidth="84.0" text="good_count" />
                <TableColumn fx:id="createDateColumn" prefWidth="180.0" text="create_date" />
            </columns>
        </TableView>
        <Button fx:id="salesW1Btn" layoutX="409.0" layoutY="149.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="174.0" text="Send from warehouse 1">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
        <Button fx:id="salesW2Btn" layoutX="409.0" layoutY="183.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="174.0" text="Send from warehouse 2">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="salesRefreshBtn" layoutX="410.0" layoutY="25.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="172.0" text="Refresh">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
    </children>
</AnchorPane>
