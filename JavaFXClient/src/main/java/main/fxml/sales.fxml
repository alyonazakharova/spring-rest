<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="main.controller.SalesController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="salesTable" layoutX="14.0" layoutY="9.0" prefHeight="342.0" prefWidth="383.0">
            <columns>
                <TableColumn fx:id="salesIdColumn" prefWidth="63.999966621398926" text="id"/>
                <TableColumn fx:id="salesGoodIdColumn" prefWidth="62.39996337890625" text="good_id"/>
                <TableColumn fx:id="salesGoodCountColumn" prefWidth="83.20001220703125" text="good_count"/>
