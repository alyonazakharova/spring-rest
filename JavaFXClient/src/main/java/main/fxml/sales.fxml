            </columns>
        </TableView>
        <Button fx:id="salesW1Btn" layoutX="410.0" layoutY="52.0" mnemonicParsing="false" prefHeight="25.0"
                prefWidth="174.0" text="Send from warehouse 1"/>
        <Button fx:id="salesW2Btn" layoutX="410.0" layoutY="92.0" mnemonicParsing="false" prefHeight="26.0"
                prefWidth="174.0" text="Send from warehouse 2"/>
